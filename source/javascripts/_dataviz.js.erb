$( document ).ready(function() {

  app.draw = draw

  function draw (obj) {
    app.dataviz = new Viz(obj)
    app.dataviz.init()
  }

  var samsung_onboarding = {
    'dataset': <%= data.samsung_onboarding.to_json %>,
    'margin': {top: 20, right: 600, bottom: 20, left: 300},
    'depth': 240,
    'image_width': 80,
    'width': 2000,
    'height': 2000
  }

  var samsung_settings = {
    'dataset': <%= data.samsung_settings.to_json %>,
    'margin': {top: 20, right: 600, bottom: 20, left: 300},
    'depth': 240,
    'image_width': 80,
    'width': 2000,
    'height': 2000
  }

  draw(samsung_settings)

});
